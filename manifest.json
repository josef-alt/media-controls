{
	"manifest_version": 3,
	"name": "controller",
	"description": "Modifies the behavior of Chrome's built in media player.",
	"version": "1",
	"icons": {
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"host_permissions": [
		"file:///*"
	],
	"content_scripts": [
	{
		"matches": ["file:///*"],
		"js": ["script.js"]
	}]
}